{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"allowJs": false,
		"declaration": false,
		"declarationMap": false,
		"sourceMap": true,
		"rootDir": "./app",
		"removeComments": true,
		"noEmit": false,
		"isolatedModules": false,
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true
	}
}